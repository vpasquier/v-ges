{
  "name": "Switch",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "description": "Switch",
  "icons": {
    "16": "images/v-16.png",
    "128": "images/v-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "chrome-bkg.js"
    ],
    "persistent": true
  },
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "https://ajax.googleapis.com/",
    "notifications",
    "background",
    "declarativeContent",
    "<all_urls>",
    "webRequest"
  ],
  "omnibox": { "keyword" : "l" },
  "commands": {
    "switch": {
      "suggested_key": {
        "default": "Ctrl+I",
        "mac": "Command+I"
      },
      "description": "Switch"
    }
  },
  "page_action": {
    "default_icon": {
      "19": "images/v-19.png",
      "38": "images/v-38.png"
    },
    "default_title": "Switch",
    "default_popup": "popup.html"
  }
}